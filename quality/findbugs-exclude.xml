<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- FindBugs doesn't like the resource classes provided by both ourselves, and the android
         support libraries. -->
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.*Test"/>
    </Match>
</FindBugsFilter>
